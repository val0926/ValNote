{
  "name": "blog-admin",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "latest",
    "ace-builds": "^1.4.12",
    "antd": "^4.6.2",
    "axios": "^0.21.1",
    "react": "^16.13.1",
    "react-ace": "^9.1.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/*",
    "update": "npm install && (npm run build; rm -rf ../public/admin); mv ./build ../public && mv ../public/build ../public/admin",
    "update2": "npm install && npm run build && rmdir /Q /S ..\\public\\admin & mkdir ..\\public\\admin && Xcopy /E /I /Q .\\build ..\\public\\admin"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-undef": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.1.1",
    "source-map-explorer": "^2.5.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "proxy": "http://localhost:3000",
  "homepage": "/admin"
}
